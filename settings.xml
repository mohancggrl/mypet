  <!-- ========================================================= -->
  <servers>
    <!--
    <server>
      <id>artifactory</id>
      <username>user</username>
      <password>password</password>
    </server>
    -->
  </servers>

  <!-- ========================================================= -->
  <!-- Mirrors (optional) -->
  <!-- ========================================================= -->
  <mirrors>
